{
  "name": "formik-antd-components",
  "version": "0.0.0-development",
  "author": "Jannik Buschke",
  "description": "Utility components for formik and ant-design",
  "displayName": "formik-antd-components",
  "keywords": [
    "react",
    "formik",
    "form",
    "utils"
  ],
  "sideEffects": false,
  "license": "MIT",
  "private": false,
  "files": [
    "lib",
    "src",
    "es"
  ],
  "module": "./es/index.js",
  "main": "./lib/index.js",
  "types": "./lib/types.d.ts",
  "scripts": {
    "tsc": "tsc && tsc -p tsconfig.es.json",
    "watch": "tsc --watch",
    "cleanbuild": "rm -r node_modules && npm install && tsc",
    "prettier": "prettier --write src/**/*.* package.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "semantic-release": "semantic-release --branch next --no-ci"
  },
  "dependencies": {
    "@types/react-transition-group": "^4.2.3"
  },
  "peerDependencies": {
    "formik": ">= 1.4.1 < 2",
    "react": ">= 16.8.0 < 17",
    "react-dom": ">= 16.8.0 < 17",
    "react-sortable-hoc": ">= 1.0.0 < 2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-info": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.11",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "formik-antd": "1.6.0",
    "antd": "3.26.4",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-prettier": "3.1.1",
    "formik": "2.0.8",
    "prettier": "1.18.2",
    "react": "16.10.2",
    "react-docgen-typescript-loader": "^3.3.0",
    "react-dom": "16.10.2",
    "tslib": "1.10.0",
    "typescript": "3.7.4",
    "react-transition-group": "4.3.0",
    "semantic-release": "^17.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jannikbuschke/formik-antd-components.git"
  }
}
